{
	"$schema": "vscode://schemas/color-theme",
    "name": "Prosper Theme",
	"colors": {
		"editor.foreground": "#eeffff",
		"sideBarTitle.foreground": "#bbbbbb",
		"focusBorder": "#5f8021",
        "selection.background": "#3e5220",
        "list.activeSelectionBackground": "#3e5220",
        "list.hoverBackground": "#1a1a1b",
        "list.inactiveSelectionBackground": "#252526",
        "activityBar.background": "#252526",
        "activityBar.border": "#252526",
        "activityBar.activeBackground": "#3e5220",
        "activityBar.activeBorder": "#9cd82b",
        "activityBarBadge.background": "#9cd82b",
        "activityBarBadge.foreground": "#141415", 
        "sideBar.background": "#141415",
        "sideBar.border": "#252526",
        "sideBarSectionHeader.background": "#141415",
        "sideBarSectionHeader.border": "#252526",
        "sideBarTitle.background": "#141415",
        "button.background": "#9cd82b",
        "button.hoverBackground": "#8cc228",
        "button.foreground": "#141415",
        "input.background": "#1a1a1b",
        "input.border": "#1a1a1b",
        "editorGroup.border": "#252526",
        "editorGroupHeader.tabsBackground": "#252526",
        "editorGroupHeader.tabsBorder": "#252526",
        "tab.activeBackground": "#141415",
        "tab.activeBorder": "#141415",
        "tab.inactiveBackground": "#1a1a1b",
        "tab.border": "#252526",
        "tab.hoverBackground": "#141415",
        "tab.activeBorderTop": "#9cd82b",
        "editor.background": "#141415",
		"scrollbar.shadow": "#141415",
        "panel.background": "#141415",
        "panel.border": "#252526",
        "panelTitle.activeBorder": "#9cd82b",
        "statusBar.background": "#252526",
        "statusBar.border": "#252526",
        "titleBar.activeBackground": "#252526",
        "titleBar.inactiveBackground": "#252526",
        "titleBar.border": "#252526",
        "menu.background": "#252526",
        "menu.border": "#252526",
        "commandCenter.background": "#1a1a1b",
        "commandCenter.activeBackground": "#141415",
        "commandCenter.activeBorder": "#141415",
        "commandCenter.border": "#2a2a2a",
        "statusBarItem.remoteBackground": "#9cd82b",
        "statusBarItem.remoteForeground": "#141415",
        "statusBarItem.remoteHoverBackground": "#8cc228",
        "statusBarItem.remoteHoverForeground": "#141415",
        "scmGraph.foreground1": "#9cd82b",
        "scmGraph.foreground2": "#8cc228",
        "scmGraph.foreground3": "#141415",
        "scmGraph.historyItemRefColor": "#9cd82b",
        "scmGraph.historyItemRemoteRefColor": "#5f8021",
        "scmGraph.historyItemBaseRefColor": "#5f8021",
        "textLink.foreground": "#9cd82b",
        "textLink.activeForeground": "#8cc228",
        "list.errorForeground": "#ef6d69",
        "list.warningForeground": "#efb55d"
	},
	"include": "./dark_plus.json",
	"tokenColors": [
        {
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#808080"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars",
				"source.powershell variable.other.member",
				"entity.name.operator.custom-literal"
			],
			"settings": {
				"foreground": "#e2dc80"
			}
		},
		{
			"name": "Constants and enums",
			"scope": [
				"variable.other.constant",
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#5ebfff",
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.namespace",
				"entity.other.attribute",
				"entity.name.scope-resolution",
				"entity.name.class",
				"storage.type.numeric.go",
				"storage.type.byte.go",
				"storage.type.boolean.go",
				"storage.type.string.go",
				"storage.type.uintptr.go",
				"storage.type.error.go",
				"storage.type.rune.go",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
                "foreground": "#4ec9a0"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class",
				"punctuation.separator.namespace.ruby"
			],
			"settings": {
                "foreground": "#4ec9a0"
			}
		},
		{
			"name": "Control flow / Special keywords",
			"scope": [
				"keyword.control",
				"source.cpp keyword.operator.new",
				"keyword.operator.delete",
				"keyword.other.using",
				"keyword.other.directive.using",
				"keyword.other.operator",
				"entity.name.operator"
			],
			"settings": {
				"foreground": "#ce8ae2"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable",
				"entity.name.variable",
				"constant.other.placeholder", // placeholders in strings
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"source.css variable",
				"source.coffee.embedded"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"scope": [
				"entity.name.tag.css",
				"entity.name.tag.less"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"source.css entity.other.attribute-name.class",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.parent.less",
				"source.css entity.other.attribute-name.pseudo-class",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#da8e70"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#da8e70"
			}
		},
		{
			"scope": [
				"string",
				"meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#da8e70"
			}
		},
		{
			"scope": "string.tag",
			"settings": {
				"foreground": "#da8e70"
			}
		},
		{
			"scope": "string.value",
			"settings": {
				"foreground": "#da8e70"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#bddf90"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#bddf90"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#bddf90"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#bddf90"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#bddf90"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": [
				"storage.modifier",
				"keyword.operator.noexcept"
			],
			"settings": {
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": [
				"constant.character",
				"constant.other.option"
			],
			"settings": {
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": [
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#5e8bdd"
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#5e8bdd"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#5e8bdd"
			}
		}
	],
  	"semanticHighlighting": true,
	"semanticTokenColors": {
		"newOperator":"#ce8ae2",
		"stringLiteral":"#da8e70",
		"customLiteral": "#e2dc80",
		"numberLiteral": "#bddf90",
	}
}
